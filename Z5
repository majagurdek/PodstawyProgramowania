#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() 
{
    int n;
    scanf("%d", &n);
    int *tab = (int*)malloc(n *sizeof(int));
    int suma = 0;
    for(int i=0; i<n; i++)
    {
        scanf("%d",&tab[i]);
        suma = suma + tab[i];
    }
    printf("%d",suma); 

    return 0;
}
