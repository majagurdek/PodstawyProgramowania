#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() 
{
    char c;
    char string[100];
    char sen[100];
    
    scanf("%c ", &c);
    scanf("%s ", string);
    fgets(sen, 100, stdin);
    
    printf("%c\n", c);
    printf("%s\n", string);
    printf("%s\n", sen);
    return 0;
}
